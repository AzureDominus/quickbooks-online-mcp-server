{
    "name": "@qboapi/qbo-mcp-server",
    "version": "0.0.1",
    "description": "MCP server implementation for QuickBooks Online integration",
    "license": "MIT",
    "main": "dist/index.js",
    "type": "module",
    "bin": "./dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && shx chmod +x dist/*.js",
        "prepare": "npm run build",
        "watch": "tsc --watch",
        "test": "node --test --experimental-strip-types --import tsx src/tests/**/*.test.ts",
        "test:unit": "node --test --experimental-strip-types --import tsx src/tests/unit/*.test.ts",
        "test:integration": "node --test --experimental-strip-types --import tsx src/tests/integration/*.test.ts",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/",
        "format:check": "prettier --check src/",
        "auth": "node dist/auth-server.js"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.6.0",
        "dotenv": "^16.4.7",
        "intuit-oauth": "^4.0.0",
        "node-quickbooks": "^2.0.43",
        "open": "^9.1.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/node": "^22.13.10",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "globals": "^16.0.0",
        "prettier": "3.5.3",
        "shx": "^0.3.4",
        "tsx": "^4.21.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
    }
}
